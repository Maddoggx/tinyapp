<header>
  <nav class="navbar navbar-expand-md navbar-light" style="background-color: #8FB682">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <% if (user) { %>
      <h5 class="px-2"> Logged in as: <%= user.email %> </h5>
      <form method="POST" action="/logout"><button type="submit"class="btn btn-light">logout</button>
        
      </form>
      <% }else{ %>
        <a class="btn btn-info" style="margin-right: 1em" href="/register" role="button">Register</a>
          <a class="btn btn-secondary" style="margin-right: 1em" href="/login" role="button">Login</a>
      <%} %>
  </nav>
</header>